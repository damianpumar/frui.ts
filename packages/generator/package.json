{
  "name": "@frui.ts/generator",
  "publishConfig": {
    "access": "public"
  },
  "version": "999.0.0",
  "description": "Frui.ts code generator",
  "keywords": [
    "code-generator"
  ],
  "homepage": "https://github.com/eManPrague/frui.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eManPrague/frui.ts.git"
  },
  "bugs": {
    "url": "https://github.com/eManPrague/frui.ts/issues"
  },
  "author": "Augustin Å ulc <sulc.gusta@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "fruits-generate": "./dist/index.js"
  },
  "scripts": {
    "start-openapi": "ts-node src/index.ts openapi -o temp/apitest",
    "clean": "rimraf dist",
    "build": "tsc",
    "postbuild": "ncp dist/src dist -o && ncp src/openapi/templates dist/openapi/templates && rimraf dist/src && rimraf dist/package.json"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^9.0.1",
    "cli-progress": "^3.8.2",
    "commander": "^5.1.0",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.15",
    "openapi-types": "^1.3.5",
    "ts-morph": "^7.1.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.7.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2"
  }
}
